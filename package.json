{
  "name": "agn-ai",
  "private": true,
  "version": "0.1.0",
  "description": "Agnostic AI Chat",
  "scripts": {
    "start": "NODE_ENV=dev concurrently -c auto -n web,api \"parcel web/index.html --no-cache\" \"npm run server:watch\"",
    "server": "node --inspect -r @swc-node/register srv/start.ts",
    "server:watch": "nodemon -e ts --watch srv --exec \"node -r @swc-node/register --inspect srv/start.ts\"",
    "start:win": "set NODE_ENV=dev && parcel src/index.html",
    "build": "NODE_ENV=production parcel build src/index.html",
    "style": "prettier web --check && prettier srv --check",
    "style:fix": "prettier web --write && prettier srv --write",
    "typecheck": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "license": "AGPL-3.0",
  "dependencies": {
    "@seald-io/nedb": "^4.0.1",
    "@solidjs/router": "^0.6.0",
    "@swc-node/register": "^1.6.1",
    "@swc/cli": "^0.1.61",
    "@swc/core": "^1.3.35",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/multiparty": "^0.0.33",
    "@types/nedb": "^1.8.12",
    "@types/needle": "^3.2.0",
    "@types/uuid": "^9.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "frisker": "^1.6.0",
    "gpt-3-encoder": "^1.1.4",
    "js-cookie": "^3.0.1",
    "lucide-solid": "0.105.0-alpha.9",
    "multiparty": "^4.2.3",
    "nedb-promises": "^6.2.1",
    "needle": "^3.2.0",
    "pino": "^8.10.0",
    "pino-pretty": "^9.2.0",
    "showdown": "^2.1.0",
    "solid-js": "^1.6.9",
    "uuid": "^9.0.0",
    "zustand": "^4.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@types/js-cookie": "^3.0.2",
    "@types/node": "^18.13.0",
    "@types/showdown": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "babel-preset-solid": "^1.6.9",
    "buffer": "^5.5.0",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "parcel": "^2.8.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "process": "^0.11.10",
    "solid-refresh": "^0.4.2",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4"
  }
}
